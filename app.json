{
  "expo": {
    "name": "ilmihal oku",
    "slug": "ilmihal-native",
    "privacy": "public",
    "platforms": ["ios", "android"],
    "jsEngine": "hermes",
    "version": "2.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.oku.ilmihal",
      "supportsTablet": true
    },
    "android": {
      "package": "com.oku.ilmihal",
      "versionCode": 11,
      "permissions": []
    }
  }
}
